{
  "name": "@acme/mobile",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "dev:awesomeproject": "turbo dev --filter=\"{./apps/awesomeproject}...\"",
    "lint": "turbo lint",
    "test": "turbo test",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "build:awesomeproject": "turbo build --filter=\"...{./apps/awesomeproject}\"",
    "prepare": "node -e \"if (process.env.NODE_ENV === 'production' || process.env.CI === 'true') process.exit(0); else process.exit(1);\" || husky"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "eslint": "9.38.0",
    "husky": "9.1.7",
    "jest": "29.6.3",
    "lint-staged": "16.2.6",
    "prettier": "3.6.2",
    "turbo": "2.6.0",
    "typescript": "5.9.3"
  },
  "packageManager": "npm@11.6.0",
  "engines": {
    "node": ">=22",
    "npm": ">=11"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
